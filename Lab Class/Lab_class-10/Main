public class Main {
    public static void main(String[] args) {
        Ball ball=new Ball("Nike");
        System.out.println("Ball's Brand Name : "+ball.getBrandName());
        ball.toss();
        ball.bounce();

        Rock rock=new Rock();
        rock.toss();

        System.out.println("\nFootball's Info - ");
        FootBall footBall=new FootBall("Adidas");
        System.out.println("Football's Brand Name : "+footBall.getBrandName());
        footBall.toss();
        footBall.bounce();

        System.out.println("\nBaseball's Info - ");
        BaseBall baseBall=new BaseBall("Marucci");
        System.out.println("BaseBall's Brand Name : "+baseBall.getBrandName());
        baseBall.toss();
        baseBall.bounce();

    }
}
